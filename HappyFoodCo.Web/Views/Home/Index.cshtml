@using HappyFoodCo.Data.Models
@model IEnumerable<Restaurant>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>HappyFoodCo</h1>
    <h3>Restaurant Dashboard</h3>

    The year is @DateTime.Now.Year.
    <div>
        There are @Model.Count() restaurants in the database.
    </div>
</div>
<ul>
    @foreach (var restaurant in Model)
    {
        <li>
            @restaurant.Name;
        </li>
    }
</ul>